{
  "name": "node-emoji",
  "version": "2.2.0",
  "description": "Friendly emoji lookups and parsing utilities for Node.js. ðŸ’–",
  "keywords": [
    "emoji",
    "simple",
    "emoticons",
    "emoticon",
    "emojis",
    "smiley",
    "smileys",
    "smilies",
    "ideogram",
    "ideograms"
  ],
  "bugs": {
    "url": "https://github.com/omnidan/node-emoji/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omnidan/node-emoji"
  },
  "license": "MIT",
  "author": {
    "name": "Daniel Bugl",
    "email": "me@omnidan.net"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./lib/index.d.ts",
        "require": "./lib/index.d.cts"
      },
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier \"**/*\" --ignore-unknown",
    "lint": "eslint . .*js --max-warnings 0 --report-unused-disable-directives",
    "lint:knip": "knip",
    "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:packages": "pnpm dedupe --check",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "prepare": "husky install",
    "should-semantic-release": "should-semantic-release --verbose",
    "test": "vitest",
    "test:cjs": "node ./src/e2e.cjs",
    "tsc": "tsc"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "@sindresorhus/is": "^7.0.2",
    "char-regex": "^2.0.2",
    "skin-tone": "^4.0.0",
    "unicode-emoji-json": "^0.8.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@swc/core": "^1.13.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "console-fail-test": "^0.5.0",
    "cspell": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-yml": "^1.18.0",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.4.0",
    "knip": "^5.62.0",
    "lint-staged": "^16.1.2",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "npm-package-json-lint": "^9.0.0",
    "npm-package-json-lint-config-default": "^8.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-curly": "^0.3.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "release-it": "^19.0.4",
    "sentences-per-line": "^0.3.0",
    "should-semantic-release": "^0.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "yaml-eslint-parser": "^1.3.0",
    "zod": "^3.25.76"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "provenance": true
  }
}
